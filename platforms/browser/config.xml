<?xml version='1.0' encoding='utf-8'?>
<widget id="info.jasonbeck" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>BeaconGame</name>
    <description>
        Beacon Bluetooth Game
    </description>
    <author email="jason.beck@nyu.edu" href="jasonbeck.info">
        Some Guy
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="phonegap-version" value="cli-6.1.0" />
</widget>
